{
  "version": 2,
  "functions": {
    "api/analyze-fish.js": {
      "memory": 1024,
      "maxDuration": 10
    },
    "api/test.js": {
      "memory": 128,
      "maxDuration": 5
    }
  },
  "routes": [
    {
      "src": "/api/analyze-fish",
      "methods": ["GET", "POST", "OPTIONS"],
      "dest": "/api/analyze-fish.js"
    },
    {
      "src": "/api/test",
      "methods": ["GET"],
      "dest": "/api/test.js"
    }
  ]
} 